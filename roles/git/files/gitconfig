[user]
	email = qzmfranklin@gmail.com
	name = Zhongming Qu
[core]
	excludesfile = ~/.gitignore
	mergeoptions = --no-edit
[push]
	default = simple
[alias]
	prunelocal = !git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -d
	prunelocal-force = !git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -D
	tree = log --graph --oneline --decorate
